{
  "name": "tokenspy",
  "displayName": "Tokenspy",
  "version": "0.0.1",
  "description": "A basic tokenspy extension.",
  "author": "birch",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.10.0",
    "@solana/web3.js": "^1.91.7",
    "daisyui": "^4.10.2",
    "framer-motion": "^11.1.7",
    "helius-sdk": "^1.2.5",
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-aria-components": "^1.1.1",
    "react-dom": "18.2.0",
    "react-use-websocket": "^4.8.1",
    "tailwindcss": "3.4.1",
    "tailwindcss-react-aria-components": "^1.1.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "$FIREFOX_EXT_ID"
      }
    }
  }
}
